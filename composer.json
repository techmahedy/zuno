{
  "name": "zuno/zuno",
  "type": "package",
  "description": "The Zuno framework",
  "license": "MIT",
  "keywords": [
    "framework",
    "zuno"
  ],
  "homepage": "https://zuno.com",
  "support": {
    "issues": "https://github.com/techmahedy/zuno/issues",
    "source": "https://github.com/techmahedy/zuno"
  },
  "authors": [
    {
      "name": "mahedi hasan",
      "email": "mahedy150101@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "monolog/monolog": "^3.0",
    "phpmailer/phpmailer": "^6.9",
    "ramsey/collection": "^2.1",
    "robmorgan/phinx": "^0.16.1",
    "symfony/console": "^7.2",
    "symfony/process": "^7.2",
    "symfony/var-dumper": "^7.1",
    "vlucas/phpdotenv": "^5.5",
    "nesbot/carbon": "^3.8"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Zuno\\": "src/zuno/"
    },
    "files": [
      "src/zuno/Helpers/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "5.x-dev"
    }
  },
  "suggest": {
    "ext-pdo": "Required to use all database features.",
    "fakerphp/faker": "Required to use the eloquent factory builder (^1.9.1)."
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
